{
  "name": "kusha.me",
  "version": "3.0.0",
  "description": "## Instructions * Clone repo * Make sure you have npm installed * do npm install",
  "main": "index.js",
  "scripts": {
    "//": "build prod, clone master git history, stage contents, commit, push",
    "unix-prod-build": "rm -rf dist/* && webpack -p --config ./build/webpack.prod.config.js",
    "win-prod-build": "npm run win-clean-dist && webpack -p --config ./build/webpack.prod.config.js",
    "unix-dev-build": "rm -rf dist/* && webpack --config ./build/webpack.dev.config.js --display-entrypoints",
    "win-dev-build": "npm run win-clean-dist && webpack --config ./build/webpack.dev.config.js --display-entrypoints",
    "unix-start-dev-server": "rm -rf dist/* && webpack-dev-server --config ./build/webpack.dev.config.js --content-base dist/ --history-api-fallback",
    "win-start-dev-server": "npm run win-clean-dist && webpack-dev-server --config ./build/webpack.dev.config.js --content-base dist/ --history-api-fallback",
    "unix-deploy-prod": "npm run unix-prod-build && mkdir dist/.git/ && git clone --bare $(git config --get remote.origin.url) --branch master dist/.git/ && npm run deploy-prod",
    "win-deploy-prod": "npm run win-prod-build && for /F \"delims=\" %A in ('git config --get remote.origin.url') do git clone --bare %A --branch master dist/.git/ && npm run deploy-prod",
    "deploy-prod": "cd dist && git add --all && git commit -m \"Prod Release\" && git push",
    "win-clean-dist": "(rd dist /s /q || echo dist not found, creating) && mkdir dist"
  },
  "repository": "git+https://github.com/kushagharahi/kushagharahi.github.io.git",
  "author": "Kusha Gharahi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kushagharahi/kushagharahi.github.io/issues"
  },
  "homepage": "https://github.com/kushagharahi/kushagharahi.github.io#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-vue": "^4.2.0",
    "file-loader": "^1.1.6",
    "markdown-loader": "^2.0.1",
    "node-sass": "^4.5.0",
    "prerender-spa-plugin": "^2.1.0",
    "sass-loader": "^6.0.3",
    "sitemap-webpack-plugin": "^0.5.1",
    "style-loader": "^0.20.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.7.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "keywords": [
    "Kusha",
    "Gharahi",
    "Portfolio",
