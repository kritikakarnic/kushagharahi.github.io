{
    "compilerOptions": {
        "target": "es5",
        "module": "none",
        "sourceMap": true,
        "outFile": "./scripts/app.js",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "noEmitOnError": true
    },
  "compileOnSave": true,
    "exclude": [
        "node_modules",
        "./scripts/packages/"
    ]
}